  <ApplicationArea>
    <CreationDateTime>2019-04-24T14:10:25Z</CreationDateTime> <!-- Required field -->
  </ApplicationArea>
  <DataArea>
    <Process/>
    <ProductionSchedule>
      <ProductionRequest>
        <!-- 1) Empty; no ID, no HierarchyScope, no SegmentRequirement -->
      </ProductionRequest>
      <ProductionRequest>
        <HierarchyScope>
          <EquipmentID>psc2</EquipmentID>
          <EquipmentElementLevel>ProcessCell</EquipmentElementLevel>
        </HierarchyScope>
        <SegmentRequirement>
          <!-- 2) Empty;
          no process segment ID,
          no start or end time,
          no EquipmentRequirement,
          no MaterialRequirement,
          no nested SegmentRequirement -->
        </SegmentRequirement>
      </ProductionRequest>
      <ProductionRequest>
        <SegmentRequirement>
          <EquipmentRequirement>
            <!-- 3) Nothing enclosed -->
          </EquipmentRequirement>
          <MaterialRequirement>
            <!-- 4) Nothing enclosed -->
          </MaterialRequirement>
          <MaterialRequirement>
            <Quantity>
              <QuantityString>42.5</QuantityString>
              <!-- 5) Minimal quantity: -->
              <!-- No data type -->
              <!-- No unit of measure -->
              <!-- No key -->
            </Quantity>
          </MaterialRequirement>
        </SegmentRequirement>
      </ProductionRequest>
    </ProductionSchedule>
